generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model country {
  id        String     @id  @default(uuid())
  name      String     @unique
  displayName   String?
  isActive  Boolean    @default(true)
  createdBy String
  updatedBy String?
  createdAt DateTime   @default(now())
  updatedAt DateTime   @updatedAt
  district  district[]
}

model town {
  id         String   @id  @default(uuid())
  name       String
  displayName String?
  districtId String
  isActive   Boolean  @default(true)
  createdBy  String
  updatedBy  String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  entity     entity[]
  district   district @relation(fields: [districtId], references: [id])

  @@unique([name, districtId])
}

model district {
  id        String   @id @unique @default(uuid())
  name      String   @unique
  displayName  String?
  countryId String
  isActive  Boolean  @default(true)
  createdBy String
  updatedBy String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  country   country  @relation(fields: [countryId], references: [id])
  town      town[]

  @@unique([name, countryId])
}

model site {
  id        String    @id @unique @default(uuid())
  name      String    @unique
  displayName   String?
  typeSite  SITE_TYPE @default(FIELD)
  entityId  String
  isActive  Boolean   @default(true)
  createdBy String
  updatedBy String?
  createdAt DateTime  @default(now())
  updateAt  DateTime  @updatedAt
  entity    entity    @relation(fields: [entityId], references: [id])

  @@unique([name, entityId])
}

model customer {
  id                String              @id @unique @default(uuid())
  name              String              @unique
  displayName       String?
  email             String
  phone             String
  isActive          Boolean             @default(true)
  createdBy         String
  updatedBy         String?
  createdAt         DateTime            @default(now())
  updateAt          DateTime            @updatedAt
  clientBankAccount clientbankaccount[]
}

model grade {
  id        String     @id @unique @default(uuid())
  name      String     @unique
  displayName   String?
  isActive  Boolean    @default(true)
  createdBy String
  updatedBy String?
  createdAt DateTime   @default(now())
  updateAt  DateTime   @updatedAt
  employees employee[]
}

model echelon {
  id        String     @id @unique @default(uuid())
  name      String     @unique
  displayName   String?
  isActive  Boolean    @default(true)
  createdBy String
  updatedBy String?
  createdAt DateTime   @default(now())
  updateAt  DateTime   @updatedAt
  employees employee[]
}

model category {
  id        String     @id @unique @default(uuid())
  name      String     @unique
  displayName   String?
  isActive  Boolean    @default(true)
  createdBy String
  updatedBy String?
  createdAt DateTime   @default(now())
  updateAt  DateTime   @updatedAt
  employees employee[]
}

model entity {
  id                 String              @id @unique @default(uuid())
  name               String              @unique
  displayName        String?
  localisation       String
  phone              String
  townId             String
  isActive           Boolean             @default(true)
  createdBy          String
  updatedBy          String?
  createdAt          DateTime            @default(now())
  updateAt           DateTime            @updatedAt
  article            article[]
  departements       department[]
  employees          employee[]
  towns              town                @relation(fields: [townId], references: [id])
  entityBankAccounts entitybankaccount[]
  shifts             shift[]
  sites              site[]
  suppliers          supplier[]
}

model employee {
  id                  String               @id @unique @default(uuid())
  email               String?              @unique
  phone               String?              @unique
  name                String?              @unique
  picture             String?
  functionId          String?
  gradeId             String?
  echelonId           String?
  categoryId          String?
  userId              String               @unique
  entityId            String
  createdBy           String
  updatedBy           String?
  createdAt           DateTime             @default(now())
  updatedAt           DateTime             @updatedAt
  isActive            Boolean              @default(true)
  category            category?            @relation(fields: [categoryId], references: [id])
  echelon             echelon?             @relation(fields: [echelonId], references: [id])
  entity              entity               @relation(fields: [entityId], references: [id])
  function            Renamedfunction?     @relation(fields: [functionId], references: [id])
  grade               grade?               @relation(fields: [gradeId], references: [id])
  employeePermissions employeepermission[]
  employeeRoles       employeerole[]
}

model employeepermission {
  id           String     @id @unique @default(uuid())
  employeeId   String
  permissionId String
  isActive     Boolean    @default(true)
  createdBy    String
  updatedBy    String?
  createdAt    DateTime   @default(now())
  updateAt     DateTime   @updatedAt
  employee     employee   @relation(fields: [employeeId], references: [id])
  permission   permission @relation(fields: [permissionId], references: [id])
}

model applicationpermission {
  id            String   @id @unique @default(uuid())
  applicationId String
  permissionId  String
  isActive      Boolean  @default(true)
  createdBy     String
  updatedBy     String?
  createdAt     DateTime @default(now())
  updateAt      DateTime @updatedAt
}

model employeerole {
  id         String   @id @unique @default(uuid())
  employeeId String
  roleId     String
  isActive   Boolean  @default(true)
  createdBy  String
  updatedBy  String?
  createdAt  DateTime @default(now())
  updateAt   DateTime @updatedAt
  employee   employee @relation(fields: [employeeId], references: [id])
  role       role     @relation(fields: [roleId], references: [id])
}

model permission {
  id                  String               @id @unique @default(uuid())
  permissionName      String
  displayName         String?
  description         String?
  createdBy           String
  updatedBy           String?
  createdAt           DateTime             @default(now())
  updatedAt           DateTime             @updatedAt
  isActive            Boolean              @default(true)
  employeePermissions employeepermission[]
  permissions         permissionrole[]
}

model permissionrole {
  id           String       @id @unique @default(uuid())
  permissionId String
  permission   permission   @relation(references: [id], fields: [permissionId])
  roleId       String
  role         role         @relation(references: [id], fields: [roleId])
  isActive     Boolean      @default(true)
  createdBy    String
  updatedBy    String?
  createdAt    DateTime @default(now())
  updateAt     DateTime @updatedAt
}

model role {
  id            String         @id @unique @default(uuid())
  roleName      String
  displayName   String?
  description   String?
  createdBy     String
  updatedBy     String?
  createdAt     DateTime       @default(now())
  updatedAt     DateTime       @updatedAt
  isActive      Boolean        @default(true)
  employeeRoles employeerole[]
  permissionroles   permissionrole[]
}

model application {
  id          String   @id @unique @default(uuid())
  name        String   @unique
  displayName String?  @unique
  url         String
  image       String?
  description String?
  isActive    Boolean  @default(true)
  createdBy   String
  updatedBy   String?
  createdAt   DateTime @default(now())
  updateAt    DateTime @updatedAt
}

model supplier {
  id        String   @id @unique @default(uuid())
  name      String   @unique
  displayName   String?   @unique
  email     String
  phone     String
  entityId  String
  isActive  Boolean  @default(true)
  createdBy String
  updatedBy String?
  createdAt DateTime @default(now())
  updateAt  DateTime @updatedAt
  entity    entity   @relation(fields: [entityId], references: [id])
}

model article {
  id              String          @id @unique @default(uuid())
  name            String
  displayName     String?
  price           Float           @default(0)
  quantity        Float           @default(0)
  hasTVA          Boolean         @default(false)
  type            ARTICLE_TYPE
  idEntity        String
  isActive        Boolean         @default(true)
  createdBy       String
  updatedBy       String?
  createdAt       DateTime        @default(now())
  updateAt        DateTime        @updatedAt
  idArticleFamily String?
  code            String?
  articleFamilyId article_family? @relation(fields: [idArticleFamily], references: [id])
  entity          entity          @relation(fields: [idEntity], references: [id])
}

model article_family {
  id          String    @id @unique @default(uuid())
  name        String
  displayName String?
  code        String
  description String?
  isActive    Boolean   @default(true)
  createdBy   String
  updatedBy   String?
  createdAt   DateTime  @default(now())
  updateAt    DateTime  @updatedAt
  articles    article[]
}

model shift {
  id        String   @id @unique @default(uuid())
  name      String?  @unique
  startTime String
  endTime   String
  entityId  String
  isActive  Boolean  @default(true)
  createdBy String
  updatedBy String?
  createdAt DateTime @default(now())
  updateAt  DateTime @updatedAt
  entity    entity   @relation(fields: [entityId], references: [id])

  @@unique([startTime, endTime])
}

model department {
  id        String    @id @unique @default(uuid())
  name      String
  displayName   String?   @unique
  entityId  String
  isActive  Boolean   @default(true)
  createdBy String
  updatedBy String?
  createdAt DateTime  @default(now())
  updateAt  DateTime  @updatedAt
  entity    entity    @relation(fields: [entityId], references: [id])
  services  service[]
}

model service {
  id           String     @id @unique @default(uuid())
  name         String
  displayName  String?
  departmentId String
  isActive     Boolean    @default(true)
  createdBy    String
  updatedBy    String?
  createdAt    DateTime   @default(now())
  updateAt     DateTime   @updatedAt
  department   department @relation(fields: [departmentId], references: [id])
}

model bank {
  id                String              @id @unique @default(uuid())
  name              String              @unique
  displayName       String?
  address           String
  phone             String
  isActive          Boolean             @default(true)
  createdBy         String
  updatedBy         String?
  createdAt         DateTime            @default(now())
  updateAt          DateTime            @updatedAt
  clientBankAccount clientbankaccount[]
  entityBankAccount entitybankaccount[]
}

model clientbankaccount {
  id            String   @id @unique @default(uuid())
  accountNumber String
  bankId        String
  clientId      String
  isActive      Boolean  @default(true)
  createdBy     String
  updatedBy     String?
  createdAt     DateTime @default(now())
  updateAt      DateTime @updatedAt
  bank          bank     @relation(fields: [bankId], references: [id])
  customer      customer @relation(fields: [clientId], references: [id])
}

model entitybankaccount {
  id            String   @id @unique @default(uuid())
  accountNumber String
  bankId        String
  entityId      String
  isActive      Boolean  @default(true)
  createdBy     String
  updatedBy     String?
  createdAt     DateTime @default(now())
  updateAt      DateTime @updatedAt
  bank          bank     @relation(fields: [bankId], references: [id])
  entity        entity   @relation(fields: [entityId], references: [id])
}

model Renamedfunction {
  id        String     @id @unique @default(uuid())
  name      String     @unique(map: "Function_name_key")
  displayName   String?
  isActive  Boolean    @default(true)
  createdBy String
  updatedBy String?
  createdAt DateTime   @default(now())
  updateAt  DateTime   @updatedAt
  employees employee[]

  @@map("function")
}

enum SITE_TYPE {
  HEADQUARTER
  FIELD
}

enum ARTICLE_TYPE {
  PRODUCT
  EQUIPEMENT
}
